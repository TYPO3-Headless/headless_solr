  "authors": [
    {
      "name": "Tymoteusz Motylewski",
      "role": "Head"
    },
    {
      "name": "Łukasz Uznański",
      "role": "Developer"
    },
    {
      "name": "TYPO3 PWA Initiative",
      "role": "Developer",
      "email": "extensions@macopedia.com"
    }
  ],
  "require": {
    "typo3/cms-core": "^11.5 || ^12.4"
  },
  "suggest": {
    "friendsoftypo3/headless": "^2.0 || ^4.0"
